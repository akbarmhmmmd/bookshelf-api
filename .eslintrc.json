{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": "off",
        "linebreak-style":"off",
        "no-multiple-empty-lines":"off",
        "no-trailing-spaces":"off",
        "eol-last":"off",
        "indent":"off",
        "no-unused-vars":"off",
        "quotes":"off",
        "arrow-body-style":"off",
        "keyword-spacing":"off",
        "no-undef":"off",
        "no-restricted-globals":"off",
        "comma-dangle":"off",
        "semi":"off",
        "max-len":"off"
    }
}
